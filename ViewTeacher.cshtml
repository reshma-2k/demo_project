@model IEnumerable<SchoolManagementDb.Models.Teacher>

<h2>Teacher List</h2>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
            <th>Contact</th>
            <th>Class</th>
            <th>Division</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var teacher in Model)
        {
            <tr>
                <td>@teacher.Name</td>
                <td>@teacher.Address</td>
                <td>@teacher.Contact</td>
                <td>@teacher.Class</td>
                <td>@teacher.Division</td>
            </tr>
        }
    </tbody>
</table>
